<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="hudongchuangxiang.haoche666.seller.fragment.ChaXunFWFragment">

    <include
        layout="@layout/view_title_single_bar" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_margin="15dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="220dp"
            android:orientation="horizontal">

            <RelativeLayout
                android:id="@+id/viewChaWeiXiu"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="1dp"
                android:layout_weight="1"
                android:background="@drawable/layout_selector_white_system_gray"
                android:gravity="center">

                <ImageView
                    android:id="@+id/imageView66"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/fuwu_chaweixiu" />

                <TextView
                    android:id="@+id/textView123"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageView66"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="30dp"
                    android:text="查维修"
                    android:textColor="@color/light_black"
                    android:textSize="@dimen/MoreBigText" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/viewChaWeiZhang"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="1dp"
                android:layout_weight="1"
                android:background="@drawable/layout_selector_white_system_gray"
                android:gravity="center">

                <ImageView
                    android:id="@+id/imageView67"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/fuwu_chaweizhang" />

                <TextView
                    android:id="@+id/textView124"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageView67"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="30dp"
                    android:text="查违章"
                    android:textColor="@color/light_black"
                    android:textSize="@dimen/MoreBigText" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="220dp"
            android:layout_marginTop="2dp"
            android:orientation="horizontal">

            <RelativeLayout
                android:id="@+id/viewChaChuXian"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="1dp"
                android:layout_weight="1"
                android:background="@drawable/layout_selector_white_system_gray"
                android:gravity="center">

                <ImageView
                    android:id="@+id/imageView68"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/fuwu_chachuxian" />

                <TextView
                    android:id="@+id/textView125"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageView68"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="30dp"
                    android:text="查出险"
                    android:textColor="@color/light_black"
                    android:textSize="@dimen/MoreBigText" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/viewChaXunLS"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="1dp"
                android:layout_weight="1"
                android:background="@drawable/layout_selector_white_system_gray"
                android:gravity="center">

                <ImageView
                    android:id="@+id/imageView69"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/fuwu_chaxunlishi" />

                <TextView
                    android:id="@+id/textView126"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageView69"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="30dp"
                    android:text="查询历史"
                    android:textColor="@color/light_black"
                    android:textSize="@dimen/MoreBigText" />
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>
